{
  "name": "@fratzinger/feathers-kysely",
  "description": "Feathers SQL service adapter using Kysely",
  "version": "0.0.3",
  "homepage": "https://github.com/fratzinger/feathers-kysely",
  "keywords": [
    "feathers",
    "feathers-plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fratzinger/feathers-kysely.git"
  },
  "author": {
    "name": "Frederik Schmatz"
  },
  "contributors": [],
  "bugs": {
    "url": "https://github.com/marshallswain/feathers-kysely/issues"
  },
  "engines": {
    "node": ">= 14"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "dist/**",
    "*.d.ts",
    "*.js"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
